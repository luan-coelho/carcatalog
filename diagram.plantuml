@startuml

class Car {
  -id: int
  -make: String
  -model: String
  -year: int
  -fuelType: String
  -color: String
  -mileage: double
  -price: double
  -description: String
}

class Brand {
   -id: int
   -name: String
 }

 class Category {
   -id: int
   -name: String
 }

Car "0..*" o-- "1" Brand : belongs to
Car "0..*" o-- "1" Category : belongs to

@enduml
